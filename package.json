{
  "name": "navri-aigp-node",
  "version": "1.0.0",
  "description": "Governance node (data plane) for AIGP - receives governance events from Navri and applies policies",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "migrate": "tsx src/scripts/migrate.ts"
  },
  "keywords": [
    "aigp",
    "governance",
    "navri",
    "ai-governance"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  }
}

